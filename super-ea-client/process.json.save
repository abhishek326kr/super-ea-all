{
  "apps": [
    {
      "name": "superadmin-client",
      "script": "node",
      "args": ["dist/index.js"],
      "instances": 1,
      "exec_mode": "fork",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      },
      "error_file": "/var/log/pm2/superadmin-client.err.log",
      "out_file":  "/var/log/pm2/superadmin-client.out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "max_memory_restart": "200M"
    }
  ]
}
